{
  "name": "@progress/kendo-angular-intl",
  "description": "Kendo UI Internationalization for Angular 2 components",
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "version": "1.7.1",
  "main": "dist/npm/index.js",
  "module": "dist/fesm5/index.js",
  "es2015": "dist/fesm2015/index.js",
  "esm5": "dist/es/index.js",
  "esm2015": "dist/es2015/index.js",
  "jsnext:main": "dist/es/index.js",
  "typings": "dist/es2015/index.d.ts",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "sideEffects": [
    "./locales/**/*"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e": "gulp e2e",
    "start": "gulp start",
    "lint": "gulp lint && gulp lint-docs",
    "api-check": "gulp api-check",
    "build-package": "gulp build-package",
    "semantic-release": "semantic-release pre && semantic-prerelease publish && semantic-release post"
  },
  "keywords": [
    "Kendo UI",
    "Internationalization",
    "i18n",
    "Angular"
  ],
  "peerDependencies": {
    "@angular/common": "4 - 8",
    "@angular/core": "4 - 8",
    "rxjs": "5.5 - 6"
  },
  "dependencies": {
    "@telerik/kendo-intl": "^1.3.0",
    "tslib": "^1.7.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.8.9",
    "@angular-devkit/schematics": "^0.8.9",
    "@angular/animations": "^4.3.1",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/platform-server": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@krux/condition-jenkins": "1.0.1",
    "@progress/kendo-angular-jest-preset": "^1.1.1",
    "@progress/kendo-angular-dateinputs": "^3.4.4",
    "@progress/kendo-angular-inputs": "^3.2.0",
    "@progress/kendo-angular-l10n": "^1.0.0",
    "@progress/kendo-angular-layout": "^1.0.0",
    "@progress/kendo-angular-tasks": "^13.1.0",
    "@progress/kendo-theme-default": "^2.0.0",
    "@telerik/semantic-prerelease": "^1.0.0",
    "@types/jest": "^21.1.8",
    "@types/jasmine": "2.2.34",
    "@types/zone.js": "0.0.27",
    "cldr-data": "latest",
    "codelyzer": "^3.0.0",
    "core-js": "^2.2.2",
    "cz-conventional-changelog": "^1.1.5",
    "ghooks": "^1.0.3",
    "gulp": "^3.9.0",
    "rxjs": "^5.5.0",
    "rxjs-tslint-rules": "^3.6.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "semantic-release": "^6.3.6",
    "tslint": "^5.4.3",
    "typescript": "~2.5.0",
    "validate-commit-msg": "^1.1.1",
    "zone.js": "0.8.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "jest": {
    "preset": "@progress/kendo-angular-jest-preset"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-angular-intl.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "@progress": {
    "friendlyName": "Internationalization"
  },
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@telerik/semantic-prerelease/analyzeCommits",
    "generateNotes": "@telerik/semantic-prerelease/generateNotes",
    "getLastRelease": "@telerik/semantic-prerelease/getLastRelease",
    "verifyConditions": "@telerik/semantic-prerelease/verifyConditions",
    "verifyRelease": "@telerik/semantic-prerelease/verifyRelease"
  },
  "schematics": "./schematics/collection.json"
}