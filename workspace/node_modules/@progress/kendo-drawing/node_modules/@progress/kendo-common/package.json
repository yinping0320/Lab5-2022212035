{
  "name": "@progress/kendo-common",
  "description": "Kendo UI TypeScript package for common functions",
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "version": "1.0.2",
  "keywords": [
    "Kendo UI",
    "TypeScript"
  ],
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "es2015": "dist/es2015/main.js",
  "typings": "dist/npm/main.d.ts",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "scripts": {
    "test": "jest",
    "start": "gulp start",
    "api": "gulp api",
    "lint": "gulp lint",
    "api-check": "gulp api-check",
    "build-package": "cross-env NODE_OPTIONS=--openssl-legacy-provider gulp build-package build-es2015-bundle",
    "semantic-release": "semantic-release pre && semantic-prerelease publish --public && semantic-release post"
  },
  "dependencies": {
    "tslib": "^1.7.0"
  },
  "devDependencies": {
    "@progress/kendo-typescript-tasks": "cdn-bundle",
    "@telerik/semantic-prerelease": "^1.0.0",
    "@types/jest": "^20.0.0",
    "colors": "^1.1.2",
    "core-js": "^2.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^1.1.5",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "ghooks": "^1.0.3",
    "gulp": "^4.0.0",
    "jest": "^23.6.0",
    "jest-preset-typescript": "^1.2.0",
    "semantic-release": "^6.3.6",
    "ts-jest": "^23.10.1",
    "typescript": "^3.4.5",
    "validate-commit-msg": "^1.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "jest": {
    "preset": "jest-preset-typescript"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-common.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@telerik/semantic-prerelease/analyzeCommits",
    "generateNotes": "@telerik/semantic-prerelease/generateNotes",
    "getLastRelease": "@telerik/semantic-prerelease/getLastRelease",
    "verifyConditions": "@telerik/semantic-prerelease/verifyConditions",
    "verifyRelease": "@telerik/semantic-prerelease/verifyRelease"
  }
}