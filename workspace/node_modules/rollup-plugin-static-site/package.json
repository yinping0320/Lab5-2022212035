{
  "name": "rollup-plugin-static-site",
  "version": "0.0.3",
  "description": "generate html out of thin air (or with any templating engine) for your static site bundle",
  "main": "lib/index.js",
  "module": "lib/index.module.js",
  "repository": "gitlab:thekelvinliu/rollup-plugin-static-site",
  "author": "kelvin liu <kelvin@thekelvinliu.com>",
  "license": "MIT",
  "files": [
    "lib",
    "README.md"
  ],
  "keywords": [
    "html",
    "rollup",
    "rollup-plugin",
    "static",
    "static-site",
    "site",
    "template"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf .nyc_output coverage lib",
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "prepublish": "yarn build",
    "reset": "rm -rf node_modules yarn.lock",
    "pretest": "[[ -r lib/index.js ]] || yarn build",
    "test": "ava",
    "test:cov": "nyc --reporter html ava"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "dot": "^1.1.2",
    "fs-extra": "^7.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-ava": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "is-builtin-module": "^2.0.0",
    "jsdoc-to-markdown": "^4.0.1",
    "lint-staged": "^7.2.2",
    "mock-fs": "^4.6.0",
    "nyc": "^12.0.2",
    "rollup": "^0.64.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-filesize": "^4.0.1",
    "tempy": "^0.2.1"
  },
  "optionalDependencies": {
    "ejs": "^2.6.1",
    "lodash.template": "^4.4.0",
    "mustache": "^2.3.2",
    "pug": "^2.0.3"
  }
}
