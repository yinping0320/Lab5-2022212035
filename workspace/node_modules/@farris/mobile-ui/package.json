{
  "name": "@farris/mobile-ui",
  "version": "0.0.22",
  "scripts": {
    "serve": "cross-env TYPE=serve webpack-dev-server --config ./website/webpack.config.js",
    "build": "yarn build:system-prod&&yarn build:theme",
    "build:all": "yarn build:bundle&&yarn build:components&&yarn build:theme",
    "build:bundle": "yarn build:umd-dev&&yarn build:umd-prod&&yarn build:esm-dev&&yarn build:esm-prod&&yarn build:system-dev&&yarn build:system-prod",
    "build:theme": "gulp build --gulpfile build/gulpfile.js",
    "build:components": "node ./build/bin.js",
    "build:website": "cross-env NODE_ENV=production webpack --config ./website/webpack.config.js",
    "build:system-dev": "cross-env NODE_ENV=development ROLLUP_FORMAT=system rollup --config ./build/build.bundle.js",
    "build:system-prod": "cross-env NODE_ENV=production ROLLUP_FORMAT=system rollup --config ./build/build.bundle.js",
    "build:umd-dev": "cross-env NODE_ENV=development ROLLUP_FORMAT=umd rollup --config ./build/build.bundle.js",
    "build:umd-prod": "cross-env NODE_ENV=production ROLLUP_FORMAT=umd rollup --config ./build/build.bundle.js",
    "build:esm-dev": "cross-env NODE_ENV=development ROLLUP_FORMAT=es rollup --config ./build/build.bundle.js",
    "build:esm-prod": "cross-env NODE_ENV=production ROLLUP_FORMAT=es rollup --config ./build/build.bundle.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@farris/rollup-plugin-vue": "^0.0.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/quill": "^2.0.6",
    "@vue/babel-plugin-jsx": "^1.0.0-rc.5",
    "@vue/compiler-sfc": "^3.0.4",
    "acorn-jsx": "^5.3.1",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "^7.14.0",
    "eslint-plugin-vue": "^7.1.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "html-webpack-plugin": "^4.5.0",
    "md-loader": "^0.1.0",
    "mini-css-extract-plugin": "^1.3.1",
    "mockjs": "^1.1.0",
    "node-sass": "^4.14.1",
    "node-sass-magic-importer": "^5.3.2",
    "ora": "^5.2.0",
    "rollup": "^2.35.1",
